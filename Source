local PlayerTab = Window:CreateTab("Show Players Roles", "eye")
local Button = PlayerTab:CreateButton({
 Name = "Show Roles",
   Callback = function()
   local Players = game:GetService("Players")

local vipLabel = workspace:GetChildren()[272].HumanoidRootPart.VIPLabel
local vipTextLabel = vipLabel:FindFirstChildWhichIsA("TextLabel")

local roleColors = {
    Character = Color3.fromRGB(0, 0, 255),
    Survivor = Color3.fromRGB(0, 255, 0),
    Visitor = Color3.fromRGB(255, 0, 0)
}

local function createRoleLabel(player)
    if player.Character and player.Character:FindFirstChild("Head") then
        for _, obj in pairs(player.Character:GetDescendants()) do
            if obj.Name == "RoleLabel" then
                obj:Destroy()
            end
        end
        
        local billboard = Instance.new("BillboardGui")
        billboard.Name = "RoleLabel"
        billboard.Adornee = player.Character.Head
        billboard.Size = vipLabel.Size
        billboard.StudsOffset = vipLabel.StudsOffset
        billboard.AlwaysOnTop = vipLabel.AlwaysOnTop
        billboard.Active = vipLabel.Active
        billboard.ClipsDescendants = vipLabel.ClipsDescendants
        billboard.LightInfluence = vipLabel.LightInfluence
        billboard.MaxDistance = vipLabel.MaxDistance
        billboard.SizeOffset = vipLabel.SizeOffset
        
        local label = Instance.new("TextLabel")
        label.Size = vipTextLabel.Size
        label.Position = vipTextLabel.Position
        label.AnchorPoint = vipTextLabel.AnchorPoint
        label.Text = player.PlayerRole.Value
        label.TextColor3 = roleColors[player.PlayerRole.Value] or vipTextLabel.TextColor3
        label.BackgroundColor3 = vipTextLabel.BackgroundColor3
        label.BackgroundTransparency = vipTextLabel.BackgroundTransparency
        label.BorderColor3 = vipTextLabel.BorderColor3
        label.BorderSizePixel = vipTextLabel.BorderSizePixel
        label.TextSize = vipTextLabel.TextSize
        label.TextScaled = vipTextLabel.TextScaled
        label.TextWrapped = vipTextLabel.TextWrapped
        label.Font = vipTextLabel.Font
        label.TextStrokeColor3 = vipTextLabel.TextStrokeColor3
        label.TextStrokeTransparency = vipTextLabel.TextStrokeTransparency
        label.Parent = billboard
        
        billboard.Parent = player.Character.Head
    end
end

for _, player in pairs(Players:GetPlayers()) do
    if player ~= Players.LocalPlayer then
        createRoleLabel(player)
        player.PlayerRole.Changed:Connect(function()
            createRoleLabel(player)
        end)
        player.CharacterAdded:Connect(function()
            wait(1)
            createRoleLabel(player)
        end)
    end
end

Players.PlayerAdded:Connect(function(player)
    if player ~= Players.LocalPlayer then
        player.CharacterAdded:Connect(function()
            wait(1)
            createRoleLabel(player)
        end)
        player.PlayerRole.Changed:Connect(function()
            createRoleLabel(player)
        end)
    end
end)

 end,
})

local PlayerTab = Window:CreateTab("Cash", "circle-dollar-sign")
local Button = PlayerTab:CreateButton({
 Name = "Get Some Cash!",
   Callback = function()

    local ReplicatedStorage = game:GetService("ReplicatedStorage")
    for i = 1, 100 do
    ReplicatedStorage.EventsFolder.GetCashRewards:FireServer(90, 100) 
    wait(0.1)
end

 end,
})

local PlayerTab = Window:CreateTab("Unlock All Morphs", "lock-keyhole-open")
local Button = PlayerTab:CreateButton({
   Name = "Unlock All Morphs",
   Callback = function()
   local player = game:GetService("Players").LocalPlayer

local function patchMorphFrame(frame)
    if not frame or not frame:IsDescendantOf(game) then return end
    
    for _, btn in ipairs(frame:GetChildren()) do
        if btn:IsA("ImageButton") and btn.Name ~= "Close" then
            btn.ImageColor3 = Color3.new(1, 1, 1)
            
            for _, connection in pairs(getconnections(btn.MouseButton1Click)) do
                connection:Disable()
            end
            
            btn.MouseButton1Click:Connect(function()
                frame.Visible = false
                game.ReplicatedStorage.EventsFolder.SetMorph:FireServer(btn.Name)
            end)
            
            local label = btn:FindFirstChild("NeedLabel")
            if label then
                label.Visible = false
            end
        end
    end

    frame.ChildAdded:Connect(function(child)
        if child:IsA("ImageButton") and child.Name ~= "Close" then
            child.ImageColor3 = Color3.new(1, 1, 1)
            
            for _, connection in pairs(getconnections(child.MouseButton1Click)) do
                connection:Disable()
            end
            
            child.MouseButton1Click:Connect(function()
                frame.Visible = false
                game.ReplicatedStorage.EventsFolder.SetMorph:FireServer(child.Name)
            end)
            
            local label = child:FindFirstChild("NeedLabel")
            if label then label.Visible = false end
        end
    end)
end

local function watchMainGui(mainGui)
    if mainGui:FindFirstChild("MorphFrame") then
        patchMorphFrame(mainGui.MorphFrame)
    end

    mainGui.ChildAdded:Connect(function(child)
        if child.Name == "MorphFrame" then
            patchMorphFrame(child)
        end
    end)
end

local function initWatcher()
    local playerGui = player:WaitForChild("PlayerGui")

    for _, gui in ipairs(playerGui:GetChildren()) do
        if gui.Name == "MainGui" then
            watchMainGui(gui)
        elseif gui.Name == "MorphFrame" then
            patchMorphFrame(gui)
        end
    end

    playerGui.ChildAdded:Connect(function(gui)
        if gui.Name == "MainGui" then
            watchMainGui(gui)
        elseif gui.Name == "MorphFrame" then
            patchMorphFrame(gui)
        end
    end)
end

task.spawn(initWatcher)

task.spawn(function()
    while task.wait(1) do
        local mainGui = player.PlayerGui:FindFirstChild("MainGui")
        if mainGui then
            local morphFrame = mainGui:FindFirstChild("MorphFrame")
            if morphFrame then
                for _, btn in ipairs(morphFrame:GetChildren()) do
                    if btn:IsA("ImageButton") and btn.Name ~= "Close" then
                        btn.ImageColor3 = Color3.new(1, 1, 1)
                    end
                end
            end
        end
    end
end)

   end,
})


